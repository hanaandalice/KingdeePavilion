<resources>

    <!-- Base application theme. -->
    <!-- Base application theme. -->
    <style name="AppThemeBase" parent="BaseAppTheme">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimary</item>
        <item name="colorAccent">@color/colorPrimary</item>
        <!--<item name="actionSheetStyle">@style/ActionSheetStyleiOS7</item>-->
    </style>
    <style name="text_small">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/label_text</item>
    </style>

    <style name="text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/label_text</item>
    </style>

    <style name="hint_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/hintColor_66</item>
    </style>
    <style name="hint_text_samll">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/hintColor_66</item>
    </style>
    <style name="hint99__text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/hintColor_99</item>
    </style>
    <style name="hint99_text_samll">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/hintColor_99</item>
    </style>

    <style name="Widget.Design.AppBarLayout" parent="android:Widget">
        <item name="elevation">0dp</item>
        <item name="android:background">@color/bg_Color</item>
    </style>

    <style name="ll_h_layout_no_margin">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:paddingTop">16dp</item>
        <item name="android:paddingBottom">16dp</item>
        <item name="android:background">@color/white</item>
    </style>
    <style name="padding_10_10_16_16">
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:paddingTop">16dp</item>
        <item name="android:paddingBottom">16dp</item>
    </style>
    <style name="ll_v_layout_no_margin">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:paddingTop">16dp</item>
        <item name="android:paddingBottom">16dp</item>
        <item name="android:background">@color/white</item>
    </style>
    <style name="et_textsize_14">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/text_color</item>
        <item name="android:textColorHint">@color/hintColor_99</item>
        <item name="android:textCursorDrawable">@drawable/color_cursor</item>
        <item name="android:cursorVisible">true</item>
    </style>
    <style name="et_textsize_12">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/text_color</item>
        <item name="android:textColorHint">@color/hintColor_99</item>
        <item name="android:textCursorDrawable">@drawable/color_cursor</item>
        <item name="android:cursorVisible">true</item>
    </style>

</resources>
